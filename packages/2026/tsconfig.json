{
  "compilerOptions": {
    "module": "ES2022",
    "target": "ES2024",
    "jsx": "preserve",
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "noEmit": true
  }
}
